        {
            "uid": "secstoreglobal",
            "info": "Launch the short session of tests",
            "action": "lua://AFT#_launch_test",
            "args": {
                "trace": "secstoreglobal",
                "files": "secstoreglobal.lua"
            }
        },
        {
            "uid": "secstoreadmin",
            "info": "Launch the short session of tests",
            "action": "lua://AFT#_launch_test",
            "args": {
                "trace": "secstoreadmin",
                "files": "secstoreadmin.lua"
            }
        }
    ]
